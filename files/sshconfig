Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

Host alpha
    HostName 192.168.2.2
    Port 31337
    User dsn

Host omega
    HostName 192.168.2.3
    Port 22 
    User root 

Host git
    HostName github.com
    Port 22
    User git
    ForwardAgent True

Host home
    HostName ssh.dsnn.io
    Port 31337
    User dsn
 
