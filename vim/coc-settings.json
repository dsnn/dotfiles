{
  "codeLens.enable": true,
  "suggest.enablePreview": true,
  "suggest.autoTrigger": "always",
  "suggest.triggerAfterInsertEnter": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.snippetIndicator": "►",
  "suggest.maxPreviewWidth": 100,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "●",
  "diagnostic.hintSign": "○",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.icon.enableNerdfont": true,
  "explorer.sources": [
    { "name": "buffer", "expand": true },
    { "name": "file", "expand": true }
  ],
  "explorer.file.column.git.icon.added": "✚",
  "explorer.file.column.git.icon.deleted": "✖",
  "explorer.file.column.git.icon.renamed": "➜",
  "explorer.file.column.git.icon.unmerged": "═",
  "explorer.file.column.git.icon.untracked": "?",
  "explorer.file.column.git.icon.modified": "✹",
  "explorer.file.column.git.icon.ignored": "☒",
  "explorer.openAction.for.directory": "expandOrCollapse",
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"]
  },
  "prettier.requireConfig": true,
  "eslint.enable": true,
  "eslint.autoFixOnSave": false,
  "eslint.autoFix": false,
  "tslint.enable": true,
  "tslint.autoFixOnSave": true
}
